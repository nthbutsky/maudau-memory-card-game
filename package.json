{
  "name": "maudau-memory-card-game",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && cd ./dist && cp index.html 404.html",
    "preview": "vite preview",
    "deploy": "sh deploy.sh",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint --ext .js,.vue,.ts",
    "lint:js:fix": "npm run lint:js --fix",
    "lint:css": "stylelint '**/*.{scss,vue}'",
    "lint:css:fix": "npm run lint:css --fix"
  },
  "dependencies": {
    "@convidera-team/eslint-config-vue3-convidera": "^1.2.0",
    "@convidera-team/stylelint-config-vue-convidera": "^3.2.0",
    "@fontsource/nunito-sans": "^4.5.9",
    "@types/node": "^18.7.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "axios": "^0.27.2",
    "eslint": "^8.21.0",
    "firebase": "^9.9.2",
    "firebaseui": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "pinia": "^2.0.18",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.54.4",
    "stylelint": "^14.10.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.40.1"
  }
}
